{
  "name": "insurance-fraud-detection",
  "version": "0.1.0",
  "private": true,
  "description": "Insurance claims fraud detection infrastructure",
  "packageManager": "yarn@4.5.1",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "cdk": "cdk",
    "cdk:synth": "cd packages/infra && cdk synth",
    "cdk:deploy": "cd packages/infra && cdk deploy --all",
    "cdk:destroy": "cd packages/infra && cdk destroy --all",
    "cdk:diff": "cd packages/infra && cdk diff",
    "cdk:bootstrap": "cdk bootstrap"
  },
  "devDependencies": {
    "aws-cdk": "^2.170.0"
  }
}
